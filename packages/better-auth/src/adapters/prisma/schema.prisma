generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:.db/dev.db"
}

model User {
    id            String    @id @default(cuid())
    email         String    @unique
    emailVerified Boolean   @default(false)
    providerId    String
    accountId     String
    password      String
    Session       Session[]
}

model Session {
    id        String   @id
    userId    String
    expiresAt DateTime
    user      User     @relation(fields: [userId], references: [id])
}
